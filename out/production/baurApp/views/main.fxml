    <Tab fx:id="singleStandardTab" text="Single buried pipe">
      <content>
          <fx:include fx:id="single" source="singleView.fxml"/>
      </content>
    </Tab>
      <Tab fx:id="doubleStandardTab" text="Double buried pipe">
